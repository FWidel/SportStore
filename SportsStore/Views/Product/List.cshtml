@model ProductListViewModel


@foreach (var p in Model.Products)
{

    @await Html.PartialAsync("ProductSummary", p);

}

<div page-model="@Model.PagingInfo" page-action="List" page-class-enabled="true"
     page-class="btn" pag-class-normal="btn-secondary"
     page-class-selected="btn-primary" class="btn-group pull-right m-1"
     page-url-category="@Model.CurrentCategory"></div>